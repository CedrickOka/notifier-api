{
	"name" : "coka/notifier-client-bundle",
	"type" : "symfony-bundle",
	"license" : "MIT",
	"description" : "Notifier client symfony bundle allows send notification",
	"homepage" : "https://github.com/CedrickOka/notifier-client-bundle",
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\Notifier\\ClientBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\Notifier\\ClientBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.4",
		"coka/notifier-message" : "^1.0",
		"coka/service-discovery-bundle" : "^1.4",
		"guzzlehttp/guzzle" : "^6.0 || ^7.4",
		"psr/log" : "^1 || ^2",
		"symfony/config" : "^4.4 || ^5.4",
		"symfony/console" : "^4.4 || ^5.4",
		"symfony/dependency-injection" : "^4.4 || ^5.4",
		"symfony/http-kernel" : "^4.4 || ^5.4"
	},
	"require-dev" : {
		"symfony/framework-bundle" : "^4.4 || ^5.4",
		"symfony/phpunit-bridge" : "^4.4 || ^5.4",
		"symfony/yaml" : "^4.4 || ^5.4"
	},
	"config" : {
		"sort-packages" : true
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/notifier-client-bundle/blob/master/docs/index.md"
	}
}
